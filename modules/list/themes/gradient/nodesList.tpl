<div class="nodes-list-block">
    <div class="node-list-cap"><div class="node-list-cap-l"><div class="node-list-cap-r"><div class="node-list-cap-bg">
        <div class="node-date"><label>[str]Date[/str]:</label> [component]date[/component]</div>
        <h2 class="node-title">[component]title[/component]</h2>
    </div></div></div></div>
    <div class="node-list-body">
    <div class="node-lt"><div class="node-rt"><div class="node-t"></div></div></div>
    <div class="node-l"><div class="node-r">
        <div class="node">
            <div class="thumbnail">[component]thumbnail[/component]</div>
            <p class="node-content">[component]content[/component]</p>
            <div class="node-author">[component]author[/component]<label>[str]Author[/str]: </label></div>
            <div class="clear"></div>
            <div class="node-rate">[component]rate[/component]</div>
            <div class="read-more">[component]link[/component]</div>
            <div class="clear"></div>
        </div>
    </div></div>
    <div class="node-lb"><div class="node-rb"><div class="node-b"></div></div></div>
    </div>
</div>
<div class="nodes-list-separator"></div>
